---
import Layout from '@layouts/Layout.astro';
import Header from '@includes/Header.astro';

import CardCardBody from '@componentsBS/CardCardBody.astro';
---

<Layout>
    <header data-el="top-fixed" class="header-app navbar navbar-expand-xl">
        <Header />

    </header>

    <main data-el="main-app" class="main-app">
        <section class="container-fluid">
            <nav aria-label="breadcrumb" class="breadcrumb-app">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="/principal/"><i class="bi bi-house-fill"></i></a>
                    </li>
                    <li class="breadcrumb-item active" aria-current="page">Material de Treinamento</li>
                </ol>
            </nav>
        </section>

        <section class="container-fluid">
            <CardCardBody>

            </CardCardBody>

        </section>
    </main>
</Layout>
