---
import CardCardBodySectionTitle from '@componentsLayout/CardCardBodySectionTitle.astro'

import SelectMes from '@components/Form/SelectMes.astro'
import BtnOk from '@components/Form/BtnOk.astro'
---

<CardCardBodySectionTitle labelTitles = "Seleção">
    <form action="" method="">
        <section class="row">
            <SelectMes col="col-xl-2 col-lg-3 col-md-6 col-sm-6" />
            <BtnOk
                col="col-xl-2 col-lg-1 col-md-6 col-sm-6"
                last="sm"
                btnLast="lg"
            />
        </section>
    </form>
</CardCardBodySectionTitle>
